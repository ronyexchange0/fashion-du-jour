seDispatch()\n\n    useEffect(() => {\n        dispatch(getCats())\n    }, [dispatch]) \n  \n\n    return (\n        <section className=\"categories\">\n            <div className=\"container\">\n                <div className=\"row categories-inner\">\n                    <div className=\"col-6 pics\">\n                        <div className=\"row image-grid-cat\">\n                            <div className=\"col-8 img1\">\n                                <img className=\"img-fluid\" src={catImage1}/>\n                            </div>\n                            \n                            <div className=\"col-6 img2\">\n                                <img className=\"img-fluid\" src={catImage3}/>\n                            </div>\n\n                            <div className=\"col-4 img3\">\n                                <img className=\"img-fluid\" src={catImage2}/>\n                            </div>\n                        </div>\n                    </div>\n\n                    <div className=\"separator\"></div>\n\n                    <div className=\"col-6 content\">\n                        <h2>Categories</h2>\n                        <div className=\"cats\">\n                            {\n                                categories.map(({name, _id}) => (\n                                    <h3 key={_id} className=\"cat\"><Link to={`/shop/${_id}/any/0/any`}>{name}</Link></h3>\n                                ))\n                            }\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </section>\n    )\n}\n\nexport default Categories\n","/home/rony/RONY/BUSINESS/ecommerce-react-node/front-end/src/components/for-you/for-you.component.jsx",[],"/home/rony/RONY/BUSINESS/ecommerce-react-node/front-end/src/components/button/button.component.jsx",["298","299"],"import React from 'react';\nimport {Link} from \"react-router-dom\";\n\nimport \"./button.style.scss\";\n\nconst Button = (